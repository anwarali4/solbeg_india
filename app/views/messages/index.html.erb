  <table>
    <tr>
      <th>ID</th>
      <th>url</th>
    </tr>
    <% @messages.each do |message|%>
      <tr>
        <td><%= message.id%></td>
        <td><ul>
          <% if message.media_assests.attached? %>
            <% message.media_assests.each do |ma| %>
              <li>
                <%= image_tag ma.variant(resize: "100x100>") %>
                <span>
                  <%= link_to 'Remove', delete_attachment_message_path(ma.id), method: :get, data: { confirm: 'Are you sure?' } %>
                  <i class="fas fa-trash"></i>
                </span>
              </li>
            <% end %>
          <% end %>
        </ul></td>
      </tr>
    <% end %>
  </table>